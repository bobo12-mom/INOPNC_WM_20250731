# Development Performance Optimizations
# Copy to .env.local for development

# Disable Sentry in development for faster startup
NEXT_PUBLIC_SENTRY_ENVIRONMENT=development
SENTRY_UPLOAD_DRY_RUN=true

# Reduce Supabase connection overhead
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Development flags for faster startup
DISABLE_ESLINT_PLUGIN=true
TSC_COMPILE_ON_ERROR=true

# Experimental Next.js optimizations
NEXT_EXPERIMENTAL_APP_BUILD_CACHE=true
NEXT_EXPERIMENTAL_TURBO=true

# Reduce bundle analyzer overhead
ANALYZE=false

# Disable telemetry for faster startup
NEXT_TELEMETRY_DISABLED=1