{
  "name": "inopnc-wm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --config jest-ci.config.js",
    "test:unit": "jest --testPathIgnorePatterns='integration|api|e2e' --coverage",
    "test:integration": "jest --testPathPattern='integration'",
    "test:api": "jest --testPathPattern='api'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:critical": "tsx scripts/test-critical-features.ts",
    "test:performance": "jest --json --outputFile=test-timings.json && node scripts/analyze-test-performance.js test-timings.json",
    "test:flaky": "node scripts/detect-flaky-tests.js",
    "snapshot:save": "./scripts/save-code-snapshot.sh",
    "protect:check": "npm run test:critical",
    "protect": "./scripts/protect-files.sh",
    "unprotect": "./scripts/unprotect-files.sh",
    "postinstall": "npm run protect",
    "prepare": "husky",
    "sync:ui-guidelines": "node scripts/sync-ui-guidelines.js",
    "type-check": "node scripts/type-check.js",
    "type-check:fix": "node scripts/type-check.js --fix",
    "type-check:watch": "node scripts/type-check.js --watch",
    "type-check:parallel": "node scripts/parallel-type-check.js",
    "analyze:types": "node scripts/analyze-ts-errors.js",
    "fix:types": "node scripts/fix-common-errors.js",
    "fix:types:preview": "node scripts/fix-common-errors.js --dry-run",
    "refactor:typography": "node scripts/refactor-typography.js",
    "fix:imports": "node scripts/auto-fix-imports.js",
    "fix:all": "npm run fix:imports && npm run refactor:typography && npm run lint --fix",
    "detect:breaking": "node scripts/detect-breaking-changes.js",
    "prebuild": "npm run sync:ui-guidelines",
    "create:component": "node scripts/create-component.js",
    "validate:ui": "node scripts/validate-ui-guidelines.js",
    "task:check": "node scripts/task-ui-check.js",
    "ui:fix": "npm run validate:ui -- --fix",
    "docs:sync": "node scripts/sync-documents.js",
    "docs:verify": "node scripts/sync-documents.js --verify-only",
    "docs:changelog": "node scripts/sync-documents.js --changelog-only",
    "docs:watch": "node scripts/auto-sync-watcher.js",
    "backup": "node scripts/backup-cli.js",
    "backup:service": "node scripts/backup-service.js",
    "backup:start": "node scripts/backup-cli.js start",
    "backup:stop": "node scripts/backup-cli.js stop",
    "backup:status": "node scripts/backup-cli.js status",
    "ci:validate": "node scripts/validate-ci-setup.js",
    "ci:check": "npm run ci:validate && npm run test:critical"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@sentry/nextjs": "^10.0.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@types/cron": "^2.0.1",
    "@types/lodash": "^4.17.20",
    "autoprefixer": "^10.4.21",
    "canvas": "^3.1.2",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cron": "^4.3.3",
    "date-fns": "^4.1.0",
    "glob": "^11.0.3",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.534.0",
    "next": "14.2.3",
    "react": "^18",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18",
    "react-error-boundary": "^6.0.0",
    "recharts": "^3.1.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.20.3",
    "web-push": "^3.6.7",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/web-push": "^3.6.4",
    "chalk": "^4.1.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "msw": "^2.10.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5",
    "undici": "^7.13.0"
  }
}
